{
  "name": "vuex-alphabetical-sorter",
  "displayName": "Vuex Alphabetical Sorter",
  "description": "Auto sort alphabetically Vuex functions in Vue",
  "version": "0.0.1",
  "icon": "images/icon.png",
  "publisher": "suddham",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suddham/vuex-alphabetical-sorter"
  },
  "bugs": {
    "url": "https://github.com/suddham/vuex-alphabetical-sorter/issues",
    "email": "suddha1@gmail.com"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Formatters"],
  "activationEvents": ["onCommand:extension.sortIt"],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sortIt",
        "title": "Vuex Alphabetical Sorter"
      }
    ],
    "keybindings": [
      {
        "command": "extension.sortIt",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.60",
    "eslint": "^4.11.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
